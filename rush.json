{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",

  "rushVersion": "5.59.2",

  "pnpmVersion": "6.7.1",

  "pnpmOptions": {
    "useWorkspaces": true
  },

  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0",

  "projectFolderMinDepth": 1,
  "projectFolderMaxDepth": 2,

  "repository": {},

  "eventHooks": {
    "preRushInstall": [],

    "postRushInstall": [],

    "preRushBuild": [],

    "postRushBuild": []
  },

  "variants": [],

  "projects": [
    {
      "packageName": "react-app",
      "projectFolder": "apps/react-app"
    },
    {
      "packageName": "vue-app",
      "projectFolder": "apps/vue-app"
    }
  ]
}
